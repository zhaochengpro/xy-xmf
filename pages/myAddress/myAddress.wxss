/* pages/myAddress/myAddress.wxss */
page{
  background: rgb(243, 241, 241);
}
/**blank css***/
.blank{
  width:100%;
  height:20rpx;
}
.van-cell-babel>text{
  font-size: 25rpx;
}
.van-cell-babel>text:nth-of-type(2){
  margin-left: 20rpx;
}
.addAddress{
  position: absolute;
  top:50%;
  left:50%;
  margin-top: -150rpx;
  margin-left: -100rpx;
  width:200rpx;
  height:300rpx;
  /* background: red; */
  text-align: center;
}
.addIcon{
  position: relative;
  width: 200rpx;
  height:200rpx;
  background: #FFA500;
  border-radius: 50%;
  text-align: center;
  box-shadow: 0rpx 0rpx 20rpx rgba(0,0,0,0.6);
  overflow: hidden;
  transition: all 0.2s linear;
}
.addIcon:active{
  box-shadow: 0rpx 0rpx 30rpx rgba(7, 7, 7, 0.9);
}
.line-h,.line-v{
  position: absolute;
  top:50%;
  left:20%;
  margin-top: -2.5rpx;
  width:60%;
  border:5rpx solid white;
  border-radius: 10rpx;
  /* transform-origin: 50%; */
}
.line-h{
  transform: rotate(0deg);
}
.line-v{
  transform: rotate(90deg);
}
.addIcon-title{
  margin-top: 20rpx;
  font-weight: 900;
  font-family: "黑体";
}
.box{
  background: red;
}
